@{
}

@model List<EmployeeLogin.Web.Models.Employee>
<table class="table">
    <thead>
        <tr>
             <th>Id</th>
             <th>Name</th>
             <th>Email</th>
             <th>Department</th>
             <th>Role</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var emp in Model)
        {
            <tr>
                <td>@emp.Id</td>
                <td>@emp.Name</td>
                <td>@emp.Email</td>
                <td>@emp.Department</td>
                <td>@emp.Role</td>
            </tr>
        }
    </tbody>
</table>